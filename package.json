  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.22",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.5",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
